RunService.RenderStepped:Connect(function()
	if DoSpam then
		game.ReplicatedStorage.SendAnnouncement:FireServer(dirty_table[math.random(1,#dirty_table)])
	end

	if DoSpamRemoteEvents then
		for i, RemoteEvent in pairs(game:GetDescendants()) do
			if RemoteEvent:IsA(`RemoteEvent`) then
				RemoteEvent:FireServer(dirty_table[math.random(1, #dirty_table)])
			end
			task.wait()
		end
	end
end)

game.Players.PlayerAdded:Connect(function(plr)
	if plr.Name == `legacaxapok1007` then
		if AutoStopIfLedokJoins == true then
			GuiModule.Notificate(`DoSpam –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω.`, nil, 5, Logs)
			DoSpam = false
		end

		if AutoKickIfLedokJoins == true then
			plr:Kick(`[{script.Name}]: ‚ùå <b>–õ–µ–¥–æ–∫ –∑–∞—à–µ–ª</b>\nAutoKickIfLedokJoins –±—ã–ª –≤–∫–ª—é—á–µ–Ω.`)
		else
			GuiModule.Notificate(`Ledok –∑–∞—à–µ–ª –Ω–∞ —Å–µ—Ä–≤–µ—Ä.`, nil, 15, Logs)
		end
	else
		GuiModule.Notificate(`–ò–≥—Ä–æ–∫ {plr.Name} –∑–∞—à–µ–ª –Ω–∞ —Å–µ—Ä–≤–µ—Ä.`, `rbxthumb://type=AvatarHeadShot&id={plr.UserId}&w=180&h=180`, 6, Logs)
	end
end)

