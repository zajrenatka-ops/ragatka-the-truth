local Button_Remoyti = GuiModule.Button(Osnova)
Button_Remoyti.Text = `‚ÑπÔ∏è –†–µ–º–æ—É—Ç—ã`

local Remoyti = GuiModule.AutoSizeFrame(Osnova)
Remoyti.Visible = false

Button_Remoyti.MouseButton1Click:Connect(function()
	Remoyti.Visible = not Remoyti.Visible
end)

local Remotes_Search = GuiModule.TextBox(Remoyti)
Remotes_Search.PlaceholderText = `üîé –ü–æ–∏—Å–∫`

local RemotesScroll = GuiModule.Scrolling(Remoyti)
RemotesScroll.Size = UDim2.fromOffset(300, 500)


Remotes_Search:GetPropertyChangedSignal(`Text`):Connect(function()

	if Remotes_Search.Text == `` then

		for i, button in pairs(RemotesScroll:GetChildren()) do
			if button:IsA(`GuiObject`) then
				button.Visible = true
			end
		end

	else
		for i, button in pairs(RemotesScroll:GetChildren()) do
			if button:IsA(`GuiObject`) then
				if string.find(button.Name, Remotes_Search.Text) then
					button.Visible = true
				else
					button.Visible = false
				end
			end
		end

	end

end)

for i, RemoteEvent in pairs(game:GetDescendants()) do
	if RemoteEvent:IsA(`RemoteEvent`) then
		if RemoteEvent.Parent then
			if RemoteEvent.Parent.Name == `RobloxReplicatedStorage` or RemoteEvent.Parent.Name == `Signals` then
				continue
				
			end
		end
		local btn_RemoteActivate = GuiModule.Button()
		btn_RemoteActivate.Name = RemoteEvent.Name
		btn_RemoteActivate.Text = `{RemoteEvent.Name}\n Parent: <b>{RemoteEvent.Parent.Name}</b>`
		btn_RemoteActivate.Parent = RemotesScroll

		local textb_Remote = GuiModule.TextBox(RemotesScroll)
		textb_Remote.Name = RemoteEvent.Name
		textb_Remote.PlaceholderText = `FireServer –¥–ª—è {RemoteEvent.Name}`

		btn_RemoteActivate.MouseButton1Click:Connect(function()
			if textb_Remote.Text ~= `` then
				RemoteEvent:FireServer(textb_Remote.Text)
				textb_Remote.Text = ``
			else
				RemoteEvent:FireServer()
			end
		end)
	end
end


