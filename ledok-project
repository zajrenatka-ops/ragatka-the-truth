local Button_Tools = GuiModule.Button(Osnova)
Button_Tools.Text = `‚ÑπÔ∏è –¢—É–ª—Å—ã`

local Tools = GuiModule.AutoSizeFrame(Osnova)
Tools.Visible = false

Button_Tools.MouseButton1Click:Connect(function()
	Tools.Visible = not Tools.Visible
end)

local Search = GuiModule.TextBox(Tools)
Search.PlaceholderText = `üîé –ü–æ–∏—Å–∫`

local Scroll = GuiModule.Scrolling(Tools)

Search:GetPropertyChangedSignal(`Text`):Connect(function()

	if Search.Text == `` then

		for i, button in pairs(Scroll:GetChildren()) do
			if button:IsA(`GuiButton`) then
				button.Visible = true
			end
		end

	else

		for i, button in pairs(Scroll:GetChildren()) do
			if button:IsA(`GuiButton`) then
				if string.find(button.Name, Search.Text) then
					button.Visible = true
				else
					button.Visible = false
				end
			end
		end

	end

end)

for i, tool in pairs(game:GetDescendants()) do 
	if tool:IsA(`Tool`) then
		local Btn_tool = GuiModule.Button()
		Btn_tool.Text = `–¢—É–ª: <b>{tool.Name}</b>`
		Btn_tool.Parent = Scroll
		Btn_tool.Name = tool.Name

		Btn_tool.MouseButton1Click:Connect(function()
			tool:Clone().Parent = Player.Backpack
		end)
	end
end

