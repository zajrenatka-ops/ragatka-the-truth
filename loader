		else
			print(`–¢–´ –•–û–†–û–®–û–ï`)
		end
	end

	GuiModule.Notificate(`‚úÖ –£—Å–ø–µ—à–Ω–æ —Å–µ—Ä–≤–µ—Ä-–∑–∞–±–∞–Ω–∏–ª –≤–µ—Å—å —Å–µ—Ä–≤–µ—Ä –∫—Ä–æ–º–µ –¥—Ä—É–∑–µ–π.`, ``, 5, Logs)

	for i, Player in pairs(NonCoolDudes) do

		if Player.Name ~= game.Players.LocalPlayer.Name then
			table.insert(BlackListed, Player.Name)
			WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)
		end

	end
end)


local Button_ServerBanPlayer = GuiModule.Button(ServerBanFrame, GuiModule.Icons.OnePlayer)
Button_ServerBanPlayer.Text = `<b>–û–¥–Ω–æ–≥–æ</b> –∏–≥—Ä–æ–∫–∞`

Button_ServerBanPlayer.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)

	for i, idk in pairs(game.CollectionService:GetTagged(`DEBRIS`)) do
		idk:Destroy()
	end

	local ChooseText = GuiModule.Text(ServerBanFrame)
	ChooseText.Text = `–í—ã–±–µ—Ä–∏ –∏–≥—Ä–æ–∫–∞.`
	ChooseText:AddTag(`DEBRIS`)


	local Search = GuiModule.TextBox(ServerBanFrame)
	Search:AddTag(`DEBRIS`)
	Search.PlaceholderText = `üîé –ü–æ–∏—Å–∫`

	local AllPlayerScroll = GuiModule.Scrolling(ServerBanFrame)
	AllPlayerScroll:AddTag(`DEBRIS`)


	Search:GetPropertyChangedSignal(`Text`):Connect(function()

		if Search.Text == `` then

			for i, button in pairs(AllPlayerScroll:GetChildren()) do
				if button:IsA(`GuiButton`) then
					button.Visible = true
				end
			end

		else

			for i, button in pairs(AllPlayerScroll:GetChildren()) do
				if button:IsA(`GuiButton`) then
					if string.find(button.Name, Search.Text) then
						button.Visible = true
					else
						button.Visible = false
					end
				end
			end

		end

	end)

	for i, Player in pairs(game.Players:GetPlayers()) do
		local PlayerButton = GuiModule.Button(AllPlayerScroll)
		PlayerButton.Name = `@{Player.Name} ({Player.DisplayName})`
		PlayerButton.Text = `@{Player.Name} ({Player.DisplayName})`

		PlayerButton.MouseButton1Click:Connect(function()

			WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)

			ChooseText:Destroy()
			AllPlayerScroll:Destroy()

			GuiModule.Notificate(`‚úÖ –£—Å–ø–µ—à–Ω–æ —Å–µ—Ä–≤–µ—Ä-–∑–∞–±–∞–Ω–∏–ª {Player.Name}.`, ``, 5, Logs)
			table.insert(BlackListed, Player.Name)

		end)
	end
end)

