local Button_WhiteList = GuiModule.Button(Osnova, GuiModule.Icons.WhiteList)
Button_WhiteList.Text = `–í–∞–π—Ç–ª–∏—Å—Ç: <b>{WhiteListMode}</b>.`

Button_WhiteList.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.categoryClick)

	if WhiteListMode == `–æ—Ç–∫–ª—é—á—ë–Ω` then
		WhiteListMode = `—Ç–æ–ª—å–∫–æ –¥—Ä—É–∑—å—è`
		
		for i, Player in pairs(game.Players:GetPlayers()) do
			if Player == game.Players.LocalPlayer then continue end
			
			if Player:IsFriendsWithAsync(game.Players.LocalPlayer.UserId) == false then
				WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)
			end
		end
		
	elseif WhiteListMode == `—Ç–æ–ª—å–∫–æ –¥—Ä—É–∑—å—è` then
		WhiteListMode = `—Ç–æ–ª—å–∫–æ –≤—ã`
		
		for i, Player in pairs(game.Players:GetPlayers()) do
			if Player == game.Players.LocalPlayer then continue end

			WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)
		end

	elseif WhiteListMode == `—Ç–æ–ª—å–∫–æ –≤—ã` then
		WhiteListMode = `–æ—Ç–∫–ª—é—á—ë–Ω`
	end
	Button_WhiteList.Text = `–í–∞–π—Ç–ª–∏—Å—Ç: <b>{WhiteListMode}</b>.`

end)


Osnova.Visible = false

local RagatkaGuiVision = GuiModule.Button()
RagatkaGuiVision.Text = `–†–∞–≥–∞—Ç–∫–∞ –ì—É–∏ –í–∏–∂–Ω v2: <b>{Osnova.Visible}</b>`
RagatkaGuiVision.AnchorPoint = Vector2.new(1, 0)
RagatkaGuiVision.Position = UDim2.new(1, -6,0, 15)
RagatkaGuiVision.Parent = MainGui

RagatkaGuiVision.MouseButton1Click:Connect(function()
	Osnova.Visible = not Osnova.Visible
	if game:GetService(`RunService`):IsStudio() then
		RagatkaGuiVision.Text = `Studio Test: <b>{Osnova.Visible}</b>`
	else
		RagatkaGuiVision.Text = `Ragatka Gui Vision v{loadstring(game:HttpGet("https://raw.githubusercontent.com/zajrenatka-ops/ragatka-the-truth/main/versionnumber"))()} <b>{Osnova.Visible}</b>`
	end

	if Osnova.Visible == true then
		TweenService:Create(Osnova, InfoBoom, {Position = UDim2.new(.5,0,.5,0)}):Play()
	end
end)

game.ContentProvider:PreloadAsync({
	`rbxassetid://10066931761`,
	`rbxassetid://10066968815`,
	`rbxassetid://10128766965`,
	`rbxassetid://10066914500`,
})


for i, Player in pairs(game.Players:GetPlayers()) do
	if Player.Name == `Feretanne` then
		GuiModule.Notificate(`‚ö†Ô∏è Feretanne –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ.`, ``, 30, Logs)
	end
end

if game.PlaceId ~= 114637499948937 then
	GuiModule.Notificate(`‚ÑπÔ∏è –¢—ã –Ω–µ –≤ –ø–ª–µ–π—Å–µ @Feretanne.`, ``, 15, Logs)
else
	GuiModule.Notificate(`‚úÖ –ó–∞–≥—Ä—É–∑–∫–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞! –ê–≤—Ç–æ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –≤–∫–ª—é—á–µ–Ω–æ. <b><i>BETA</i></b>`, ``,  5, Logs)
end


game.Players.PlayerAdded:Connect(function(Player)
	
	if Player.Name == `Feretanne` then
		
		if AutoKickIfFeretanJoins then
			game.Players.LocalPlayer:Kick(`‚ö†Ô∏è Feretanne –∑–∞—à–µ–ª –Ω–∞ —Å–µ—Ä–≤–µ—Ä. –í—Å–µ —Å–µ—Ä–≤–µ—Ä-–±–∞–Ω—ã –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω—ã.`)
		else
			GuiModule.Notificate(`‚ö†Ô∏è Feretanne –∑–∞—à–µ–ª –Ω–∞ —Å–µ—Ä–≤–µ—Ä. –°—Ç–∞—Ç—É—Å –∞–≤—Ç–æ—Å—Ç–æ–ø–∞ —Å–µ—Ä–≤–µ—Ä-–±–∞–Ω–æ–≤: {BlacklistActive}`, SoundsModule.vaznoe_notif, 30, Logs)
		end

	end
	
	if NotIfNewPlr == true then
		GuiModule.Notificate(`–ò–≥—Ä–æ–∫ <b>{Player.Name}</b> –∑–∞—à–µ–ª –Ω–∞ —Å–µ—Ä–≤–µ—Ä.`, ``, 5, Logs)
	end
	
	if WhiteListMode == `—Ç–æ–ª—å–∫–æ –¥—Ä—É–∑—å—è` then
		
		if Player:IsFriendsWithAsync(game.Players.LocalPlayer.UserId) == false then
			WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)
		end
		
	elseif WhiteListMode == `—Ç–æ–ª—å–∫–æ –≤—ã` then
		
		WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)
		
	end
	
	if BlacklistActive == true then
		if table.find(BlackListed, Player.Name) then
			WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)
		end
	end

	
end)


game.Players.PlayerRemoving:Connect(function(Player, Reason)

	if NotIfNewPlr == true then
		if table.find(BlackListed, Player.Name) then
			GuiModule.Notificate(`–ò–≥—Ä–æ–∫ <b>{Player.Name}</b> –∫–∏–∫–Ω—É—Ç —Å —Å–µ—Ä–≤–µ—Ä–∞\n—Ç–∞–∫ –∫–∞–∫ –æ–Ω —Å–µ—Ä–≤–µ—Ä-–∑–∞–±–∞–Ω–µ–Ω.`, ``, 5, Logs)
		else
			GuiModule.Notificate(`–ò–≥—Ä–æ–∫ <b>{Player.Name}</b> –≤—ã—à–µ–ª —Å —Å–µ—Ä–≤–µ—Ä–∞.`, ``, 5, Logs)
		end
	end
end)


if game:GetService(`RunService`):IsStudio() == false then
	task.defer(function()
		local VisionVersion = loadstring(game:HttpGet("https://raw.githubusercontent.com/zajrenatka-ops/ragatka-the-truth/main/versionnumber"))()

		while true do
			local NewVersion = loadstring(game:HttpGet("https://raw.githubusercontent.com/zajrenatka-ops/ragatka-the-truth/main/versionnumber"))()
			
			print(`–¢–µ–∫—É—â–∞—è –≤–µ—Ä—Å–∏—è: {VisionVersion}; –ù–æ–≤–∞—è –≤–µ—Ä—Å–∏—è: {NewVersion}`)

			
			if VisionVersion ~= NewVersion then
				GuiModule.Notificate(`–î–æ—Å—Ç—É–ø–Ω–∞ –Ω–æ–≤–∞—è –≤–µ—Ä—Å–∏—è Ragatka Gui Vision: <b>{NewVersion}</b>\n–û–±–Ω–æ–≤–∏—Ç—å—Å—è –º–æ–∂–Ω–æ –≤ –≥–ª–∞–≤–Ω–æ–º –º–µ–Ω—é`, ``, 120, Logs)
				local UpdateButton = GuiModule.Button(Osnova, GuiModule.Icons.Update)
				UpdateButton.Text = `<b>–û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –¥–æ –≤–µ—Ä—Å–∏–∏ <i>{NewVersion}</i></b>.`
				UpdateButton:FindFirstChildOfClass(`UIGradient`).Color = ColorSequence.new({
					ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 230)),
					ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 0, 136))
				})
				
				UpdateButton.MouseButton1Click:Connect(function()
					
					MainGui:Destroy()
					SoundGroup:Destroy()
					
					loadstring(game:HttpGet("https://raw.githubusercontent.com/zajrenatka-ops/ragatka-the-truth/main/loader"))()
				end)
				
				break
			end
			
			task.wait(5)
		end
		
	end)
end


