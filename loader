local MainGui = Instance.new(`ScreenGui`)
MainGui.Name = `Ragatka Gui Vision V2`
MainGui.DisplayOrder = 100000
MainGui.IgnoreGuiInset = true
MainGui.ResetOnSpawn = false
MainGui.ZIndexBehavior = `Sibling`
MainGui.Parent = PlayerGui

local WorkspaceRemotes = workspace:WaitForChild(`EventsFolder`)

local SoundsModule = {
	hover = `rbxassetid://10066931761`,
	click = `rbxassetid://10066968815`,
	categoryClick = `rbxassetid://10128766965`,
	warn = `rbxassetid://10066914500`,
	notification = `rbxassetid://17582299860`,
	vaznoe_notif = `rbxassetid://117409737658405`,
}


local SoundGroup = Instance.new(`SoundGroup`)
SoundGroup.Name = `SFX_{script.Name}`
SoundGroup.Parent = game:GetService(`SoundService`)

function SoundsModule.PlaySound(SoundName:string)
	local Sound = Instance.new(`Sound`)
	Sound.SoundId = SoundName
	Sound.Parent = SoundGroup
	Sound:Play()
	Debris:AddItem(Sound, Sound.TimeLength + 1)
end

local GuiModule = {}

function GuiModule.Notificate(Text, Sound, Time, to)
	
	if Sound == `` then
		SoundsModule.PlaySound(SoundsModule.notification)
	else
		SoundsModule.PlaySound(Sound)
	end
	
	local Notification = Instance.new(`ImageButton`)
	Notification.AnchorPoint = Vector2.new(.5,.5)
	
	Notification.BackgroundTransparency = 1
	Notification.Size = UDim2.fromOffset(500,40)
	
	local Container = Instance.new(`Frame`)
	Container.BackgroundTransparency = 1
	Container.AnchorPoint = Vector2.new(0,0.5)
	Container.Size = UDim2.fromScale(1,1)
	Container.Parent = Notification

	local Label = Instance.new(`TextLabel`)
	Label.BackgroundTransparency = .4
	Label.BackgroundTransparency = 1
	Label.TextTransparency = 1
	Label.Text = Text
	--Label.Size = UDim2.fromScale(1,1)
	Label.RichText = true
	Label.TextSize = 25
	Label.AutomaticSize = `XY`
	Label.BackgroundColor3 = Color3.fromRGB(0,0,0)
	Label.Font = Enum.Font.Nunito
	Label.TextColor3 = Color3.fromRGB(255,255,255)
	Label.Parent = Container

	GuiModule.Padding(Label)
	local Corner = GuiModule.Corner(Label)
	Corner.CornerRadius = UDim.new(1,0)

	local CountdownContainer = Instance.new(`CanvasGroup`)

	CountdownContainer.ZIndex = 2
	CountdownContainer.BackgroundTransparency = 1
	CountdownContainer.Parent = Container
	GuiModule.List(CountdownContainer)

	local Corner = GuiModule.Corner(CountdownContainer)
	Corner.CornerRadius = UDim.new(1,0)

	local Gradient = Instance.new(`UIGradient`)
	Gradient.Color = ColorSequence.new{ ColorSequenceKeypoint.new(0, Color3.fromRGB(0,0,0)), ColorSequenceKeypoint.new(1, Color3.fromRGB(255,255,255)) }
	Gradient.Transparency = NumberSequence.new{ NumberSequenceKeypoint.new(0, 0), NumberSequenceKeypoint.new(1, 1) }
	Gradient.Rotation = 90
	Gradient.Parent = CountdownContainer
	
	local Countdown = Instance.new(`Frame`)
	Countdown.BackgroundTransparency = .7
	Countdown.BackgroundColor3 = Color3.fromRGB(0,0,0)
	Countdown.Size = UDim2.fromScale(1,1)
	Countdown.Parent = CountdownContainer

	Notification.Parent = to

	CountdownContainer.Size = UDim2.fromOffset(Label.AbsoluteSize.X, Label.AbsoluteSize.Y)

	local function Close()
		SoundsModule.PlaySound(SoundsModule.warn)
		
		TweenService:Create(Countdown, Info, {Size = UDim2.fromScale(0,1)}):Play()
		TweenService:Create(Notification, Info, {Size = UDim2.fromOffset(40,40)}):Play()
		TweenService:Create(Label, Info, {TextTransparency = 1}):Play()
		TweenService:Create(Label, Info, {BackgroundTransparency = 1}):Play()

		Debris:addItem(Notification, .6)
	end

	TweenService:Create(Container, InfoV2, {Position = UDim2.fromScale(0,.5)}):Play()
	TweenService:Create(Label, InfoV2, {TextTransparency = 0}):Play()
	TweenService:Create(Label, InfoV2, {BackgroundTransparency = .4}):Play()

	local Carantine = coroutine.create(function()
		TweenService:Create(Countdown, TweenInfo.new(Time), {Size = UDim2.fromScale(0,1)}):Play()

		task.wait(Time - .5)
		Close()
	end)

	coroutine.resume(Carantine)

	Notification.MouseButton1Click:Connect(function()
		coroutine.close(Carantine)
		Close()
	end)

end

function GuiModule.Corner(guiObject)
	local Corner = Instance.new(`UICorner`)
	Corner.CornerRadius = UDim.new(0,10)
	Corner.Parent = guiObject
	return Corner
end

function GuiModule.Padding(guiObject)
	local Padding = Instance.new(`UIPadding`)
	Padding.PaddingBottom = UDim.new(0,5)
	Padding.PaddingLeft = UDim.new(0,15)
	Padding.PaddingRight = UDim.new(0,15)
	Padding.PaddingTop = UDim.new(0,5)

	Padding.Parent = guiObject
	return Padding
end

function GuiModule.Stroke(guiObject)
	local Stroke = Instance.new(`UIStroke`)
	Stroke.Thickness = 1
	Stroke.Color = Color3.fromRGB(255,255,255)
	Stroke.Transparency = .5
	Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	Stroke.Parent = guiObject

	return Stroke
end

function GuiModule.Button(guiObject)
	local Button = Instance.new(`TextButton`)
	GuiModule.Corner(Button)

	Button.BackgroundColor3 = Color3.fromRGB(255,255,255)
	Button.TextSize = 22
	Button.Font = Enum.Font.Nunito
	Button.AutomaticSize = `XY`
	Button.BorderMode = `Inset`
	Button.RichText = true
	Button.Size = UDim2.fromOffset(0,0)
	Button.AutoButtonColor = false

	local Padding = GuiModule.Padding(Button)
	local Stroke = GuiModule.Stroke(Button)

	Button.MouseEnter:Connect(function()
		
		TweenService:Create(Button, Info, {BackgroundColor3 = Color3.fromRGB(213, 213, 213)}):Play()
		TweenService:Create(Padding, Info, { PaddingLeft = UDim.new(0,20), PaddingRight = UDim.new(0,20) }):Play()
		TweenService:Create(Button, InfoBack, {BorderSizePixel = 5}):Play()
	end)

	Button.MouseLeave:Connect(function()
		SoundsModule.PlaySound(SoundsModule.hover)

		TweenService:Create(Button, Info, {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
		TweenService:Create(Padding, Info, { PaddingLeft = UDim.new(0,15), PaddingRight = UDim.new(0,15) }):Play()
		TweenService:Create(Button, InfoBack, {BorderSizePixel = 0}):Play()
	end)

	Button.MouseButton1Down:Connect(function()
		TweenService:Create(Button, TweenInfo.new(.2), {BackgroundColor3 = Color3.fromRGB(172, 172, 172)}):Play()
		TweenService:Create(Padding, InfoBoom, { PaddingLeft = UDim.new(0,15), PaddingRight = UDim.new(0,15) }):Play()
		TweenService:Create(Button, InfoBoom, {BorderSizePixel = 0}):Play()
	end)

	Button.MouseButton1Click:Connect(function()
		TweenService:Create(Button, TweenInfo.new(.1), {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()

	end)

	Button.Parent = guiObject

	return Button
end

function GuiModule.Frame()
	local Osnova = Instance.new(`Frame`)
	GuiModule.Corner(Osnova)

	Osnova.BackgroundTransparency = .5
	Osnova.AnchorPoint = Vector2.new(0.5,0.5)
	Osnova.Position = UDim2.fromScale(.5,.5)
	Osnova.AutomaticSize = `XY`
	Osnova.Size = UDim2.fromOffset(0,0)
	Osnova.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Osnova.BorderSizePixel = 0
	Osnova.Parent = MainGui

	local Drag = Instance.new(`UIDragDetector`)
	Drag.Parent = Osnova

	local Padding = GuiModule.Padding(Osnova)
	Padding.PaddingTop = UDim.new(0,15)
	Padding.PaddingBottom = UDim.new(0,15)

	local List = GuiModule.List(Osnova)

	return Osnova
end

function GuiModule.List(guiObject)
	local List = Instance.new(`UIListLayout`)
	List.SortOrder = `LayoutOrder`
	List.HorizontalAlignment = `Left`
	List.Padding = UDim.new(0,5)
	List.Parent = guiObject
	return List
end

function GuiModule.Text(parentTo)
	local Text = GuiModule.Button()
	Text.Interactable = false
	Text.BackgroundTransparency = 1
	Text.TextColor3 = Color3.fromRGB(255,255,255)
	Text.Parent = parentTo
	return Text
end

function GuiModule.Icon(parentTo)
	local Icon = Instance.new(`ImageLabel`)
	Icon.Size = UDim2.fromOffset(50,50)
	GuiModule.Corner(Icon)

	return Icon
end

function GuiModule.Scrolling(parentTo)
	local Scroll = Instance.new(`ScrollingFrame`)
	Scroll.Size = UDim2.new(1,0,0,200)
	Scroll.AutomaticCanvasSize = `XY`
	Scroll.LayoutOrder = 0
	Scroll.BackgroundTransparency = 1
	Scroll.CanvasSize = UDim2.new(0,0,0,0)
	Scroll.Parent = parentTo
	GuiModule.List(Scroll)

	return Scroll
end

function GuiModule.TextBox(parentTo)

	local TextBox = Instance.new(`TextBox`)
	TextBox.TextSize = 22
	TextBox.BorderMode = `Inset`
	TextBox.Font = Enum.Font.Nunito
	TextBox.BackgroundColor3 = Color3.fromRGB(255,255,255)
	TextBox.Size = UDim2.fromOffset(0,0)
	TextBox.AutomaticSize = `XY`
	TextBox.ClearTextOnFocus = false
	TextBox.Text = ``
	TextBox.Parent = parentTo
	GuiModule.Corner(TextBox)
	local Padding = GuiModule.Padding(TextBox)
	GuiModule.Stroke(TextBox)

	TextBox.Focused:Connect(function()
		TweenService:Create(Padding, InfoV2, { PaddingLeft = UDim.new(0,10), PaddingRight = UDim.new(0,120) }):Play()
	end)

	TextBox.FocusLost:Connect(function()
		TweenService:Create(Padding, Info, { PaddingLeft = UDim.new(0,15), PaddingRight = UDim.new(0,15) }):Play()
		TweenService:Create(TextBox, InfoBack, {BorderSizePixel = 0}):Play()
	end)

	TextBox.MouseEnter:Connect(function()
		TweenService:Create(Padding, Info, { PaddingLeft = UDim.new(0,20), PaddingRight = UDim.new(0,20) }):Play()
		TweenService:Create(TextBox, InfoBack, {BorderSizePixel = 5}):Play()
	end)

	TextBox.MouseLeave:Connect(function()
		TweenService:Create(TextBox, InfoBack, {BorderSizePixel = 0}):Play()
	end)

	return TextBox
end

function GuiModule.AutoSizeFrame(parentTo)
	local Frame = GuiModule.Frame()
	Frame:FindFirstChild(`UIDragDetector`):Destroy()
	Frame.AutomaticSize = `XY`
	Frame.BackgroundTransparency = 1
	Frame.Size = UDim2.fromOffset(0,0)
	Frame.Parent = parentTo

	return Frame
end

