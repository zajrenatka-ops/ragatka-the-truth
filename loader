	Additional.Visible = not Additional.Visible
end)

local AutoStopOfFreJoinsBtn = GuiModule.Button(Additional)
AutoStopOfFreJoinsBtn.Text = `–ê–≤—Ç–æ—Å—Ç–æ–ø –ø—Ä–∏ –∑–∞—Ö–æ–¥–µ Feretanne: {BlacklistActive}`
AutoStopOfFreJoinsBtn.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)

	BlacklistActive = not BlacklistActive
	AutoStopOfFreJoinsBtn.Text = `–ê–≤—Ç–æ—Å—Ç–æ–ø –ø—Ä–∏ –∑–∞—Ö–æ–¥–µ Feretanne: {BlacklistActive}`
end)

local TellTheTruth = GuiModule.Button(Additional)
TellTheTruth.Text = `–†–∞—Å—Å–∫–∞–∑–∞—Ç—å –ø—Ä–∞–≤–¥—É –æ –∏–≥—Ä–µ`
TellTheTruth.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)

	task.spawn(function()
		while true do
			for i, Player in pairs(game.Players:GetPlayers()) do

				if Player.Name ~= game.Players.LocalPlayer.Name then
					WorkspaceRemotes.KillPlayerEvent:FireServer(Player.Name)
				end

			end
			task.wait(.05)
		end
	end)
	task.spawn(function()
		while true do
			workspace.PlayerTypingEvent:FireServer(LeaveText)
			task.wait(.05)
		end
	end)	
	
	
end)

