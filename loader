local GlobalMessageTextBox = GuiModule.TextBox(GlobalMessages)
GlobalMessageTextBox.PlaceholderText = `üí¨ –¢–µ–∫—Å—Ç —Å–æ–æ–±—â–µ–Ω–∏—è`

local GlobalMessageSend = GuiModule.Button(GlobalMessages, GuiModule.Icons.Send)
GlobalMessageSend.Text = `–û—Ç–ø—Ä–∞–≤–∏—Ç—å`
GlobalMessageSend.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)
	
	WorkspaceRemotes.MessageEvent:FireServer(GlobalMessageTextBox.Text)
end)

local GlobalMessageAutoSpam = GuiModule.Button(GlobalMessages, GuiModule.Icons.Spam)
GlobalMessageAutoSpam.Text = `–ê–≤—Ç–æ—Å–ø–∞–º: <b>{AutoSpam}</b>`
GlobalMessageAutoSpam.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)

	AutoSpam = not AutoSpam
	
	if AutoSpam == true then
		task.defer(function()
			while AutoSpam do
				
				WorkspaceRemotes.MessageEvent:FireServer(GlobalMessageTextBox.Text)

				if tonumber(SpamDelay) ~= nil then
					task.wait(SpamDelay)
				else
					task.wait(.5)
				end
			end
		end)
	end
	
	GlobalMessageAutoSpam.Text = `–ê–≤—Ç–æ—Å–ø–∞–º: <b>{AutoSpam}</b>`
end)

local Pereodichnost = GuiModule.TextBox(GlobalMessages)
Pereodichnost.PlaceholderText = `–ó–∞–¥–µ—Ä–∂–∫–∞`
Pereodichnost:GetPropertyChangedSignal(`Text`):Connect(function()
	SpamDelay = tonumber(Pereodichnost.Text)
end)

