local Osnova = GuiModule.Frame()


local KillPlayers = GuiModule.Button(Osnova, GuiModule.Icons.Peoples)
KillPlayers.Text = `–ö–∏–ª–ª –ò–≥—Ä–æ–∫–æ–≤`

local KillPlayersFrame = GuiModule.AutoSizeFrame(Osnova)
KillPlayersFrame.Visible = false

KillPlayers.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.categoryClick)
	
	KillPlayersFrame.Visible = not KillPlayersFrame.Visible
end)

local Button_KillServer = GuiModule.Button(KillPlayersFrame, GuiModule.Icons.All)
Button_KillServer.Text = `–í—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤`
Button_KillServer.MouseButton1Click:Connect(function()
	GuiModule.Notificate(`‚úÖ –£—Å–ø–µ—à–Ω–æ —É–±–∏–ª –≤–µ—Å—å —Å–µ—Ä–≤–µ—Ä.`, ``, 5, Logs)

	for i, Player in pairs(game.Players:GetPlayers()) do

		if Player.Name ~= game.Players.LocalPlayer.Name then
			WorkspaceRemotes.KillPlayerEvent:FireServer(Player.Name)
		end

	end
end)


local Button_KillNonFriends = GuiModule.Button(KillPlayersFrame, GuiModule.Icons.Friends)
Button_KillNonFriends.Text = `–í—Å–µ—Ö <b>–∫—Ä–æ–º–µ</b> –¥—Ä—É–∑–µ–π`

Button_KillNonFriends.MouseButton1Click:Connect(function()
	local NonCoolDudes = {}
	
	GuiModule.Notificate(`ü§î –°–æ—Å—Ç–∞–≤–ª—è—é —Å–ø–∏—Å–æ–∫`, ``, 3, Logs)

	for i, Player in pairs(game.Players:GetPlayers()) do
		if Player:IsFriendsWithAsync(game.Players.LocalPlayer.UserId) == false then
			table.insert(NonCoolDudes, Player)
		else
		end
	end
	
	GuiModule.Notificate(`‚úÖ –£—Å–ø–µ—à–Ω–æ —É–±–∏–ª –≤–µ—Å—å —Å–µ—Ä–≤–µ—Ä –∫—Ä–æ–º–µ –¥—Ä—É–∑–µ–π.`, ``, 5, Logs)
	
	for i, Player in pairs(NonCoolDudes) do
		
		if Player.Name ~= game.Players.LocalPlayer.Name then
			WorkspaceRemotes.KillPlayerEvent:FireServer(Player.Name)
		end

	end
end)

local Button_KillPlayer = GuiModule.Button(KillPlayersFrame, GuiModule.Icons.OnePlayer)
Button_KillPlayer.Text = `<b>–û–¥–Ω–æ–≥–æ</b> –∏–≥—Ä–æ–∫–∞`

Button_KillPlayer.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)
	
	for i, idk in pairs(game.CollectionService:GetTagged(`DEBRIS`)) do
		idk:Destroy()
	end
	
	local ChooseText = GuiModule.Text(KillPlayersFrame)
	ChooseText.Text = `–í—ã–±–µ—Ä–∏ –∏–≥—Ä–æ–∫–∞.`
	ChooseText:AddTag(`DEBRIS`)
	

	local Search = GuiModule.TextBox(KillPlayersFrame)
	Search:AddTag(`DEBRIS`)
	Search.PlaceholderText = `üîé –ü–æ–∏—Å–∫`

	local AllPlayerScroll = GuiModule.Scrolling(KillPlayersFrame)
	AllPlayerScroll:AddTag(`DEBRIS`)


	Search:GetPropertyChangedSignal(`Text`):Connect(function()

		if Search.Text == `` then

			for i, button in pairs(AllPlayerScroll:GetChildren()) do
				if button:IsA(`GuiButton`) then
					button.Visible = true
				end
			end

		else

			for i, button in pairs(AllPlayerScroll:GetChildren()) do
				if button:IsA(`GuiButton`) then
					if string.find(button.Name, Search.Text) then
						button.Visible = true
					else
						button.Visible = false
					end
				end
			end

		end

	end)
	
	for i, Player in pairs(game.Players:GetPlayers()) do
		local PlayerButton = GuiModule.Button(AllPlayerScroll)
		PlayerButton.Name = Player.Name
		PlayerButton.Text = Player.Name
		
		PlayerButton.MouseButton1Click:Connect(function()
			
			WorkspaceRemotes.KillPlayerEvent:FireServer(Player.Name)

			ChooseText:Destroy()
			AllPlayerScroll:Destroy()
			
			GuiModule.Notificate(`‚úÖ –£—Å–ø–µ—à–Ω–æ —É–±–∏–ª {Player.Name}.`, ``, 5, Logs)
			
		end)
	end
end)


