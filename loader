	Search:GetPropertyChangedSignal(`Text`):Connect(function()

		if Search.Text == `` then

			for i, button in pairs(AllPlayerScroll:GetChildren()) do
				if button:IsA(`GuiButton`) then
					button.Visible = true
				end
			end

		else

			for i, button in pairs(AllPlayerScroll:GetChildren()) do
				if button:IsA(`GuiButton`) then
					if string.find(button.Name, Search.Text) then
						button.Visible = true
					else
						button.Visible = false
					end
				end
			end

		end

	end)

	for i, Player in pairs(game.Players:GetPlayers()) do
		local PlayerButton = GuiModule.Button(AllPlayerScroll)
		PlayerButton.Name = `@{Player.Name} ({Player.DisplayName})`
		PlayerButton.Text = `@{Player.Name} ({Player.DisplayName})`

		PlayerButton.MouseButton1Click:Connect(function()

			WorkspaceRemotes.KickPlayerEvent:FireServer(Player.Name)

			ChooseText:Destroy()
			AllPlayerScroll:Destroy()

			GuiModule.Notificate(`‚úÖ –£—Å–ø–µ—à–Ω–æ —Å–µ—Ä–≤–µ—Ä-–∑–∞–±–∞–Ω–∏–ª {Player.Name}.`, ``, 5, Logs)
			table.insert(BlackListed, Player.Name)

		end)
	end
end)

-- –°–ï–†–í–ï–†-–†–ê–ó–ë–ê–ù

local Button_ServerUnBan = GuiModule.Button(Osnova)
Button_ServerUnBan.Text = `‚ÑπÔ∏è –†–∞–∑–±–∞–Ω`

local ServerUnBan = GuiModule.AutoSizeFrame(Osnova)
ServerUnBan.Visible = false

Button_ServerUnBan.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.categoryClick)

	ServerUnBan.Visible = not ServerUnBan.Visible
	
	if ServerUnBan.Visible == false then return end
	
	for i, idk in pairs(game.CollectionService:GetTagged(`DEBRIS`)) do
		idk:Destroy()
	end
	
	local BannedPlayersList = GuiModule.Scrolling(ServerUnBan)
	BannedPlayersList:AddTag(`DEBRIS`)
	
	for i, bannedPlayer in pairs(BlackListed) do
		local unbanBtn = GuiModule.Button(BannedPlayersList)
		unbanBtn.Text = bannedPlayer

		unbanBtn.MouseButton1Click:Connect(function()
			unbanBtn:Destroy()
			GuiModule.Notificate(`‚úÖ –£—Å–ø–µ—à–Ω–æ —Å–µ—Ä–≤–µ—Ä-—Ä–∞–∑–±–∞–Ω–∏–ª {bannedPlayer}.`, ``, 5, Logs)

			table.remove(BlackListed, table.find(BlackListed, bannedPlayer))
		end)
	end
end)

local UnServerBanTextBox = GuiModule.TextBox(ServerUnBan)
UnServerBanTextBox.PlaceholderText = `–ù–∏–∫...`

local UnServerBanButton = GuiModule.Button(ServerUnBan)
UnServerBanButton.Text = `–†–∞–∑–±–∞–Ω–∏—Ç—å`

UnServerBanButton.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)

	table.remove(BlackListed, table.find(BlackListed, UnServerBanTextBox.Text))
end)

local UnServerBanEveryone = GuiModule.Button(ServerUnBan)
UnServerBanEveryone.Text = `–†–∞–∑–±–∞–Ω–∏—Ç—å –≤—Å–µ—Ö`
UnServerBanEveryone.MouseButton1Click:Connect(function()
	SoundsModule.PlaySound(SoundsModule.click)

	BlackListed = {}
end)


